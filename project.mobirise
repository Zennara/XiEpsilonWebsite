          "_name": "design-block",
          "_sourceTheme": "mobirise5",
          "_styles": {
            "padding-top": "(@paddingTop * 15px)",
            "padding-bottom": "(@paddingBottom * 15px)",
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value"
            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)"
            },
            ".container-fluid": {
              "padding": "0 3rem"
            },
            ".underline": {
              ".line": {
                "width": "5rem",
                "height": "3px",
                "background": "linear-gradient(to right, @primaryColor 50%, @secondaryColor 120%)",
                "display": "inline-block"
              }
            },
            ".timeline-text-content": {
              "padding": "2rem 2.5rem",
              "margin-left": "2rem",
              "text-align": "left",
              "p": {
                "margin-bottom": "0"
              }
            },
            ".time-line-date-content": {
              "margin-right": "2rem",
              "p": {
                "padding": "2rem 1.4rem",
                "float": "right"
              }
            },
            ".timeline-element": {
              "margin-bottom": "50px",
              "position": "relative",
              "word-wrap": "break-word",
              "word-break": "break-word",
              "display": "flex",
              "-webkit-flex-direction": "row",
              "flex-direction": "row"
            },
            ".iconBackground": {
              "position": "absolute",
              "left": "25%",
              "width": "20px",
              "height": "20px",
              "line-height": "30px",
              "text-align": "center",
              "border-radius": "50%",
              "border": "2px solid @timelineColor",
              "font-size": "30px",
              "display": "inline-block",
              "top": "29px",
              "margin-left": "-10px"
            },
            ".separline:before, .first-separline:before": {
              "top": "3rem",
              "bottom": "0",
              "position": "absolute",
              "content": "\"\"",
              "width": "2px",
              "background-color": "@timelineColor",
              "left": "calc(25% ~\"-\" 1px)",
              "height": "calc(100% ~\"+\" 2rem)"
            },
            ".separline:after": {
              "top": "calc(100% ~\"+\" 6.1rem)",
              "bottom": "0",
              "position": "absolute",
              "content": "\"\"",
              "width": "2px",
              "background-color": "@timelineColor",
              "left": "calc(25% ~\"-\" 1px)",
              "height": "100%"
            },
